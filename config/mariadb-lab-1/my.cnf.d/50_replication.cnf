[mysqld]

skip-slave-start
log-slave-updates
binlog-format               = ROW
sync_binlog                 = 1
expire_logs_days            = 3
# log-bin                     = /data2/mysql/binlog/mysql-bin
# relay-log                   = /data2/mysql/relaylog/mysql-relay
max_binlog_size             = 128M
slave_compressed_protocol   = 1
slave_parallel_threads      = 8
slave_parallel_mode         = conservative

